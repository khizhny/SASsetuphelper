    <string name="app_name">GSM Alarm</string>
    <string name="action_settings">Settings</string>
    <string name="hint_text">During alarm phone call you can use such keys:\n
        1 - Alarm ON\n
        2 - Alarm OFF\n
        3 - Microphone ON\n
        4 - Microphone OFF\n
        5 - Speaker ON\n
        6 - Speaker OFF\n
        7 - Siren ON\n
        8 - Siren OFF\n
        9 - Relay ON\n
        0 - Relay OFF\n
        * - Stop calling/texting other numbers
    </string>
